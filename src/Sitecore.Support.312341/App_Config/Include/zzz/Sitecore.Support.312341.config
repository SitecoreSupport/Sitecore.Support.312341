<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:exmEnabled="http://www.sitecore.net/xmlconfig/exmEnabled/">
  <sitecore exmEnabled:require="yes" role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <exm>
      <plainTextTokens type="Sitecore.Support.AlternativeTextTokens, Sitecore.Support.312341">
        <tokens hint="raw:AddTokenMapping">
          <token name="{unsubscribeAll}">/sitecore%20modules/Web/EXM/UnsubscribeFromAll.aspx</token>
        </tokens>
      </plainTextTokens>
    </exm>
    <pipelines>
      <SendEmail>
        <processor type="Sitecore.EmailCampaign.Cm.Pipelines.SendEmail.FillEmail, Sitecore.EmailCampaign.Cm" resolve="true">
          <patch:attribute name="type">Sitecore.Support.EmailCampaign.Cm.Pipelines.SendEmail.FillEmail, Sitecore.Support.312341</patch:attribute>
        </processor>
      </SendEmail>
    </pipelines>
  </sitecore>
</configuration>